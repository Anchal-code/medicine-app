<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@color/green">

    <View
        android:id="@+id/ellipse_7"
        android:layout_width="281dp"
        android:layout_height="281dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="192dp"
        android:layout_marginTop="-104dp"
        android:background="@drawable/ellipse7" />

    <View
        android:id="@+id/ellipse_8"
        android:layout_width="281dp"
        android:layout_height="281dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="-162dp"
        android:layout_marginTop="491dp"
        android:background="@drawable/ellipse8" />

    <View
        android:id="@+id/ellipse_9"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="228dp"
        android:layout_marginTop="177dp"
        android:background="@drawable/ellipse9" />

    <View
        android:id="@+id/ellipse_10"
        android:layout_width="11dp"
        android:layout_height="11dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="46dp"
        android:layout_marginTop="491dp"
        android:background="@drawable/ellipse10" />

    <View
        android:id="@+id/rectangle_4"
        android:layout_width="316dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="230dp"
        android:background="@drawable/rectangle46" />

    <View
        android:id="@+id/rectangle47"
        android:layout_width="316dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="22dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="286dp"
        android:background="@drawable/rectangle47"
        />



    <com.google.android.material.button.MaterialButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Log In"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="127dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="356dp"
        android:background="@drawable/rectangle37"
        android:textColor="@color/green"
        android:textAllCaps="false"
        android:textAppearance="@style/log_in"
        />


    <TextView
        android:id="@+id/email"
        android:layout_width="32dp"
        android:layout_height="15dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="57dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="242dp"
        android:text="@string/email"
        android:textAppearance="@style/email"
        android:lineSpacingExtra="0sp"
        android:gravity="top"
        />

    <TextView
        android:id="@+id/password"
        android:layout_width="57dp"
        android:layout_height="15dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="57dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="298dp"
        android:text="@string/password"
        android:textAppearance="@style/password"
        android:lineSpacingExtra="0sp"
        android:gravity="top"
        />




</RelativeLayout>